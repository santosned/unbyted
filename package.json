{
  "name": "unbyted",
  "description": "Turn bytes into readable measurement units.",
  "version": "0.3.1",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./@types/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .ts --fix",
    "build": "rollup --config --bundleConfigAsCjs",
    "dev": "ts-node ./package/index.ts",
    "dev:types": "tsc --watch --emitDeclarationOnly --module commonjs --declarationDir out",
    "test": "jest",
    "test:lint": "eslint . --ext .ts --format unix --max-warnings 1",
    "test:format": "prettier --check .",
    "prepack": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/santosned/unbyted.git"
  },
  "keywords": [
    "unbyted",
    "bytes",
    "bits",
    "pretty",
    "format",
    "convert",
    "transform",
    "decimal",
    "binary",
    "measurement",
    "units",
    "human",
    "readable",
    "typescript",
    "javascript",
    "browser",
    "node"
  ],
  "author": "E. M. Santos <santosned@proton.me>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/santosned/unbyted/issues"
  },
  "homepage": "https://github.com/santosned/unbyted#readme",
  "devDependencies": {
    "@jest/globals": "^29.3.1",
    "@rollup/plugin-terser": "^0.3.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-standard-with-typescript": "^31.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-tsdoc": "^0.2.17",
    "jest": "^29.3.1",
    "prettier": "^2.8.3",
    "rollup": "^3.10.1",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4"
  }
}
