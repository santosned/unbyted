{
  "trailingComma": "es5",
  "tabWidth": 2,
  "semi": true,
  "singleQuote": true,
  "quoteProps": "as-needed",
  "printWidth": 120,
  "overrides": [
    {
      "files": "*.md",
      "options": {
        "semi": false
      }
    },
    {
      "files": "*.yml",
      "options": {
        "quoteProps": "consistent",
        "singleQuote": true
      }
    }
  ]
}
